<% include partials/_pk_header %>

<div class="container mt-5">
    <div class="row">
        <div class="col-md-7">
            <table class="table table-bordered table-hover">
                <thead>
                <tr>
                    <td>#</td>
                    <td>Category</td>
                    <td>Details</td>   
                    <td> Urls </td>                 
                </tr>
                </thead>
                <tbody>
                <% if (data) { %>
                    <% for(var i = 0; i < data.length; i++) { %>
                        <tr>
                            <td><%= (i + 1) %></td>
                            <td><%= data[i].Catagory%></td>
                            <td><%= data[i].Details %></td>
                            <td><%= data[i].Url %></td>
                            <td>
                                <a href="/Know/update/<%= data[i].id %>" class="btn btn-info">Edit</a>
                                <a href="/Know/delete/<%= data[i].id %>" class="btn btn-danger">Delete</a>
                            </td>
                        </tr>
                    <% } %>
                <% } %>
                </tbody>
            </table>
        </div>
        
    </div>
</div>

<% include partials/_pk_footer %>
